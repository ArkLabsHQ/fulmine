templ LightningIcon() {
  <svg xmlns="http://www.w3.org/2000/svg" class="h-4 w-4 mx-auto" viewBox="0 0 48 48">
    <path fill="none" stroke="currentColor" stroke-linejoin="round" stroke-width="4" d="M19 4h18L26 18h15L17 44l5-19H8L19 4Z"></path>
  </svg>
}

templ Tab(name, active string, icon templ.Component) {
	<a href={ templ.URL("/" + strings.ToLower(name)) }>
		<p class={ "py-2", "px-4", templ.KV("active", active == strings.ToLower(name)) }>
			@icon
			<span class="ml-2">{ name }</span>
		</p>
	</a>
}

templ Tabs(active string) {
	<style>
    #tabs p {
			border-bottom: 1px solid #666;
			&.active {
        border-bottom: 1px solid #ccc;
		  }
    }
		#tabs svg {
			display: inline;
		}
  </style>
	<div class="grid grid-cols-4 text-center" id="tabs">
	  @Tab("History", active, HistoryIcon())
	  @Tab("ASP", active, AspIcon())
	  @Tab("Lightning", active, LightningIcon())
	  @Tab("Settings", active, SettingsIcon())
	</div>
}


